
use('practica2');


const aggregation = [
  {
    $group: {
      _id: '$idHabitacion',
      Habitacion: {
        $first: '$Habitacion'
      },
      count: {
        $sum: 1
      }
    }
  },
  {
    $sort: 
    {
      count: -1
    }
  },
  {
    $limit: 5
  }
]


db.LogActividades.aggregate(aggregation);
